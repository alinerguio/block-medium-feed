{
    "name": "Block Medium Feed",
    "version": "1.0",
    "description": "Block Medium Feed",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["background.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Open the popup"
    },
    "permissions": [ 
          "scripting",
          "tabs",
          "activeTab"
      ],
      "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
      }
  }